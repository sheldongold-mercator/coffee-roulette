# Frontend Environment Configuration
# Copy this file to .env and fill in your values

# API Configuration
REACT_APP_API_URL=http://localhost:3000

# Microsoft MSAL Configuration
# These should match your Azure AD app registration
REACT_APP_MSAL_CLIENT_ID=your_client_id_here
REACT_APP_MSAL_TENANT_ID=your_tenant_id_here
REACT_APP_MSAL_REDIRECT_URI=http://localhost/auth/callback

# Application Name
REACT_APP_NAME=Coffee Roulette
